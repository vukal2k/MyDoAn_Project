@section Scripts {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-datetimepicker/2.5.20/jquery.datetimepicker.full.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-datetimepicker/2.5.20/jquery.datetimepicker.js"></script>
    <script>
        function SelectFacilityType(option, id = "") {
            if (option.value == "Room") {
                $('#RoomSize' + id).prop('disabled', false);
                $('#RoomCapacity' + id).prop('disabled', false);
                $('#RoomConnectivity' + id).prop('disabled', false);
                $('#EquipmentAvailable' + id).prop('disabled', false);
            }
            else {
                $('#RoomSize' + id).prop('disabled', true);
                $('#RoomCapacity' + id).prop('disabled', true);
                $('#RoomConnectivity' + id).prop('disabled', true);
                $('#EquipmentAvailable' + id).prop('disabled', true);
            }
            if (option.value == "Internet Access") {
                $('#WirelessAccessInformation' + id).prop('disabled', false);
            }
            else {
                $('#WirelessAccessInformation' + id).prop('disabled', true);
            }
        }
        function LocationOpenDay(_day, _startTime, _endTime) {
            this.day = _day;
            this.startTime = _startTime;
            this.endTime = _endTime;

            return this;
        }

        $(document).ready(function () {
            var openDays = JSON.parse($("#OpenDay").val());
            for (var i = 0; i < openDays.length; i++) {
                if (openDays[i].day == 'Monday') {
                    $('#mondayCheck').prop('checked', true);
                    $('#mondayStartTime').val(openDays[i].startTime)
                    $('#mondayEndTime').val(openDays[i].endTime)
                    continue;
                }

                if (openDays[i].day == 'Tuesday') {
                    $('#tuesdayCheck').prop('checked', true);
                    $('#tuesdayStartTime').val(openDays[i].startTime)
                    $('#tuesdayEndTime').val(openDays[i].endTime)
                    continue;
                }

                if (openDays[i].day == 'Wednesday') {
                    $('#wednesdayCheck').prop('checked', true);
                    $('#wednesdayStartTime').val(openDays[i].startTime)
                    $('#wednesdayEndTime').val(openDays[i].endTime)
                    continue;
                }

                if (openDays[i].day == 'Thursday') {
                    $('#thursdayCheck').prop('checked', true);
                    $('#thursdayStartTime').val(openDays[i].startTime)
                    $('#thursdayEndTime').val(openDays[i].endTime)
                    continue;
                }

                if (openDays[i].day == 'Friday') {
                    $('#fridayCheck').prop('checked', true);
                    $('#fridayStartTime').val(openDays[i].startTime)
                    $('#fridayEndTime').val(openDays[i].endTime)
                    continue;
                }

                if (openDays[i].day == 'Saturday') {
                    $('#saturdayCheck').prop('checked', true);
                    $('#saturdayStartTime').val(openDays[i].startTime)
                    $('#saturdayEndTime').val(openDays[i].endTime)
                    continue;
                }

                if (openDays[i].day == 'Sunday') {
                    $('#sundayCheck').prop('checked', true);
                    $('#sundayStartTime').val(openDays[i].startTime)
                    $('#sundayEndTime').val(openDays[i].endTime)
                    continue;
                }
            }
        });
        function LoadMinorWorkStatus() {
            var ActualStartDatePart = $("#ActualStartDate").val().split("/");
            var ActualCompletionDatePart = $("#ActualCompletionDate").val().split("/");
            if (new Date(ActualStartDatePart[2], ActualStartDatePart[1] - 1, ActualStartDatePart[0]) > new Date()) {
                $("#MWStatus").val("Work Not Started")
                return;
            }
            if (new Date(ActualStartDatePart[2], ActualStartDatePart[1] - 1, ActualStartDatePart[0]) <= new Date() && ActualCompletionDatePart.length != 3) {
                $("#MWStatus").val("Work in Progress")
                return;
            }
            if (new Date(ActualCompletionDatePart[2], ActualCompletionDatePart[1] - 1, ActualCompletionDatePart[0]) < new Date()) {
                $("#MWStatus").val("Work Completed")
                return;
            }
            $("#MWStatus").val("")
        }
        function ValidateMinorWork(id = "") {
            var valid = true;
            var ActualStartDatePart = $("#ActualStartDate" + id).val().split("/");
            var ActualStartDate = new Date(ActualStartDatePart[2], ActualStartDatePart[1] - 1, ActualStartDatePart[0]);

            var ActualCompletionDatePart = $("#ActualCompletionDate" + id).val().split("/");
            var ActualCompletionDate = new Date(ActualCompletionDatePart[2], ActualCompletionDatePart[1] - 1, ActualCompletionDatePart[0]);

            var EnquiryReceiveDatePart = $("#EnquiryReceiveDate" + id).val().split("/");
            var EnquiryReceiveDate = new Date(EnquiryReceiveDatePart[2], EnquiryReceiveDatePart[1] - 1, EnquiryReceiveDatePart[0]);

            var AnticipatedCompletionPart = $("#AnticipatedCompletion" + id).val().split("/");
            var AnticipatedCompletion = new Date(AnticipatedCompletionPart[2], AnticipatedCompletionPart[1] - 1, AnticipatedCompletionPart[0]);

            var AuthorisedDatePart = $("#AuthorisedDate" + id).val().split("/");
            var AuthorisedDate = new Date(AuthorisedDatePart[2], AuthorisedDatePart[1] - 1, AuthorisedDatePart[0]);

            if (AuthorisedDate < EnquiryReceiveDate) {
                $("#AuthorisedDateError" + id).val("Authorised Date must be >= Enquiry Received Date")
                valid = false;
            }
            if (ActualStartDate < AuthorisedDate) {
                $("#ActualStartDateError" + id).val("Actual Start Date must be >= Authorised Date")
                valid = false;
            }
            if (AnticipatedCompletion < EnquiryReceiveDate) {
                $("#AnticipatedCompletionError" + id).val("Anticipated Completion Date must be >= Enquiry Received Date ")
                valid = false;
            }
            if (ActualCompletionDate < ActualStartDate) {
                $("#ActualCompletionDateError" + id).val("Actual Complete Date must be >= Actual Start Date ")
                valid = false;
            }
            return valid;
        }
        function ValidateVolunteering(id = "") {
            var valid = true;
            var StartDatePart = $("#volunteeringStartDate" + id).val().split("/");
            var StartDate = new Date(StartDatePart[2], StartDatePart[1] - 1, StartDatePart[0]);

            var EndDatePart = $("#volunteeringEndDate" + id).val().split("/");
            var EndDate = new Date(EndDatePart[2], EndDatePart[1] - 1, EndDatePart[0]);

            if (EndDate > StartDate) {
                $("#volunteeringStartError").val("End Date must not be before Start Date")
            }

            return valid;
        }
